@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix ll:    <http://ll-plugins.nongnu.org/lv2/namespace#> .

<https://michaelganger.org/plugins/cnn_distortion>
	a lv2:Plugin ;
	lv2:binary <distortion.so> ;
	doap:name "CNN Distortion" ;
	doap:license <https://opensource.org/licenses/ISC> ;
	lv2:project <https://michaelganger.org/plugins/> ;
	lv2:minorVersion 0 ;
	lv2:microVersion 1 ;
	lv2:optionalFeature lv2:hardRTCapable ;
	ll:pegName "p" ;

	lv2:port [
		a lv2:AudioPort, lv2:InputPort ;
		lv2:index 0 ;
		lv2:symbol "in" ;
		lv2:name "In"
	] , [
		a lv2:AudioPort, lv2:OutputPort ;
		lv2:index 1 ;
		lv2:symbol "out" ;
		lv2:name "Out"
	] , [
		a lv2:ControlPort , lv2:InputPort ;
		lv2:index 2 ;
		lv2:symbol "gain" ;
		lv2:name "Gain" ;
		lv2:minimum -90.0 ;
		lv2:maximum 24.0 ;
		lv2:default 0.0 ;
		units:unit units:db ;
		lv2:scalePoint [
			rdfs:label "+5" ;
			rdf:value 5.0
		] , [
			rdfs:label "0" ;
			rdf:value 0.0
		] , [
			rdfs:label "-5" ;
			rdf:value -5.0
		] , [
			rdfs:label "-10" ;
			rdf:value -10.0
		] ;
	] , [
		a lv2:ControlPort , lv2:InputPort ;
		lv2:index 3 ;
		lv2:symbol "makeup" ;
		lv2:name "Makeup Gain" ;
		lv2:minimum -90.0 ;
		lv2:maximum 24.0 ;
		lv2:default 0.0 ;
		units:unit units:db ;
		lv2:scalePoint [
			rdfs:label "+5" ;
			rdf:value 5.0
		] , [
			rdfs:label "0" ;
			rdf:value 0.0
		] , [
			rdfs:label "-5" ;
			rdf:value -5.0
		] , [
			rdfs:label "-10" ;
			rdf:value -10.0
		] ;
	] .
